{
    "name": "alikhaleghi/adminpanel",
    "description": "Admin Panel",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "codeigniter"
    ],
    "authors": [
        {
            "name": "Ali Khaleghi",
            "email": "awli.khaleghi@gmail.com",
            "homepage": "http://ahur.ir",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/alikhaleghi/adminpanel",
    "require": {
        "alikhaleghi/basesys": "@dev",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "codeigniter4/codeigniter4-standard": "^1.0",
        "codeigniter4/devkit": "^1.0",
        "codeigniter4/framework": "^4.1",
        "myth/auth": "^1.1",
        "mockery/mockery": "^1.0"
    },
    "provide": {
        "codeigniter4/authentication-implementation": "1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AliKhaleghi\\AdminPanel\\": "src"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "scripts": { }
}
